{{!Viewport}}
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no" />
{{!Autor en google}}
<link rel="author" href="{{data.google-plus}}" />
{{!--web sharing--}}
	{{#if front.share-img}}
		<link href="{{front.share-img}}" rel="image_src" />
	{{else}}
		<link href="{{data.share-img}}" rel="image_src" />
	{{/if}}

	{{!--TWITTER CARD--}}
	<meta name="twitter:card" content="summary_large_image" />
	{{!--OPEN GRAPH--}}
	{{#if front.share-description}}
		<meta property="og:title" content="{{data.share-title}}" />
	{{else}}
		<meta property="og:title" content="{{front.share-title}}" />
	{{/if}}
	<meta property="og:type" content="Website" />
	
	

	{{#if front.share-description}}
		<meta property="og:description" content="{{front.share-description}}" />
	{{else}}
		<meta property="og:description" content="{{data.share-description}}" />
	{{/if}}

	{{#if front.share-url}}
		<meta property="og:url" content="{{front.share-url}}" />
	{{else}}
		<meta property="og:url" content="{{data.url}}" />
	{{/if}}

	{{#if front.share-title}}
		<meta property="og:site_name" content="{{front.share-title}}" />
	{{else}}
		<meta property="og:site_name" content="{{data.share-title}}" />
	{{/if}}

	{{#if front.share-img}}
		<meta property="og:image" content="{{front.share-img}}" />
	{{else}}
		<meta property="og:image" content="{{data.share-img}}" />
	{{/if}}
	{{!--manifest--}}
	<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
	<link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
	<link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
	<link rel="manifest" href="/manifest.json">
	<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
	<meta name="apple-mobile-web-app-title" content="Maravillarte">
	<meta name="application-name" content="Maravillarte">
	<meta name="theme-color" content="#ffffff">
	